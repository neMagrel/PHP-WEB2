{
    "name": "my/composer-practice",
    "description": "Лабораторная работа №13 по основам Composer",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "monolog/monolog": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}